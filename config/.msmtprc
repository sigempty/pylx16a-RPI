# content of /home/user/.msmtprc
# do: `$ chmod 600 /home/user/.msmtprc`
#
# gmail
account sigempty@gmail.com # Please replace with your username
host smtp.gmail.com	   # Please replace with your email server (if you are not using gmail)
port 587
tls on
tls_starttls on
auth on
user sigempty		 # Please replace with your username 
from sigempty@gmail.com  # Please replace with your email address

# https://myaccount.google.com/apppasswords
password [your_password] # Please replace with your password (no bracket) 

# default account
account default : sigempty@gmail.com # Please replace with your email address 
